{
  "name": "quickstart-express-react",
  "version": "1.0.0",
  "description": "Multi-page application quickstart for Express + React",
  "scripts": {
    "start": "bash -c 'NODE_ENV=production node index.js'",
    "clean": "bash -c 'rm -rf public/{js,css}'",
    "dev:js": "node index dev",
    "dev:css": "gulp scss:watch",
    "dev": "npm-run-all clean -p dev:*",
    "build:js": "webpack",
    "build:css": "gulp scss:build",
    "compress": "bash -c 'gzip -9 -q public/**/*.{css,js,map}'",
    "build": "npm-run-all clean -p build:* -s compress",
    "tool": "node index"
  },
  "keywords": [
    "quickstart",
    "express",
    "react"
  ],
  "author": "Ionut-Cristian Florescu <ionut.florescu@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-latest-minimal": "^1.1.2",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "lowdb": "^0.14.0",
    "mime": "^1.3.4",
    "morgan": "^1.7.0",
    "underscore-db": "^0.12.2",
    "vash": "^0.12.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "axios": "^0.15.3",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "browser-sync": "^2.18.6",
    "cssnano": "^3.10.0",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "gulp": "^3.9.1",
    "gulp-postcss": "^6.2.0",
    "gulp-sass": "^3.0.0",
    "gulp-sourcemaps": "^1.9.1",
    "moment": "^2.17.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  }
}
